"*":
  "atom-beautify":
    executables:
      php:
        path: "/usr/bin/php"
      "php-cs-fixer":
        path: "/usr/local/bin/php-cs-fixer"
    general: {}
    js:
      break_chained_methods: true
      end_with_comma: true
      space_in_paren: true
    latex:
      indent_preamble: true
      remove_trailing_whitespace: true
    php: {}
    scss:
      align_assignments: true
      convert_quotes: "single"
      no_lead_zero: true
    vue:
      brace_style: "end-expand"
      break_chained_methods: true
      end_with_comma: true
      indent_inner_html: true
      object_curly_spacing: true
      space_in_paren: true
      unescape_strings: true
  "atom-ide-ui":
    use:
      "atom-ide-code-format": "always"
      "atom-ide-code-highlight": "always"
      "atom-ide-datatip": "always"
      "atom-ide-definitions": "always"
      "atom-ide-find-references": "always"
      "atom-ide-outline-view": "always"
      "atom-ide-refactor": "always"
      hyperclick: "always"
  "atom-latex":
    build_after_save: false
  "atom-runner":
    paneSplitDirection: "Down"
  "atom-updater-linux":
    checkStartup: false
  "autoclose-html": {}
  core:
    audioBeep: false
    closeDeletedFileTabs: true
    disabledPackages: [
      "welcome"
      "background-tips"
      "wrap-guide"
      "vim-mode-plus"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._* Thumbs.db"
      "desktop.ini"
    ]
    telemetryConsent: "no"
  editor:
    preferredLineLength: 150
    softWrap: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "101d3433-a94b-4314-a510-30811b1d19b5"
  "ide-php":
    phpPath: "/usr/bin/php"
  "markdown-preview":
    useGitHubStyle: true
  minimap:
    plugins:
      pigments: true
      pigmentsDecorationsZIndex: 0
  phpunit:
    displayInTextBuffer: true
  "platformio-ide-terminal":
    style:
      theme: "dracula"
  "spell-check":
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.html.php.blade"
    ]
    locales: [
      "en-US"
    ]
  "terminal-plus":
    style:
      theme: "dracula"
  "tree-view": {}
  "vim-mode-plus":
    automaticallyEscapeInsertModeOnActivePaneItemChange: true
    clearMultipleCursorsOnEscapeInsertMode: true
    findAcrossLines: true
    hideCommandsFromCommandPalette: true
    useSmartcaseForFind: true
  wordcount:
    currencysymbol: "â‚¬"
    goal: 1500
    ignorecode: true
    noextension: true
    showprice: true
    wordprice: "0.10"
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
